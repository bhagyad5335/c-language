#include<stdio.h>
void main()
{
    int r,c,i,j;
    int sum,a[100][100];
    scanf("%d",&r);
    scanf("%d",&c);
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
        for(i=0;i<r;i++)
        {
            sum=0;
            for(j=0;j<c;j++)
            {
        sum=sum+a[i][j];
        }
    printf("%d row sum=%d\n",i+1,sum);
    }
}
